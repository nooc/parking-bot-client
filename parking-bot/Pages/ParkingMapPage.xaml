<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:map="clr-namespace:Mapsui.UI.Maui;assembly=Mapsui.UI.Maui"
             x:Class="ParkingBot.Pages.ParkingMapPage"
             xmlns:vm="clr-namespace:ParkingBot.ViewModels"
             x:DataType="vm:ParkingMapPageVm"
             Title="Parking Map">
    <VerticalStackLayout
        VerticalOptions="FillAndExpand"
        HorizontalOptions="FillAndExpand">
        
        <map:MapControl x:Name="MapCtrl"
            VerticalOptions="FillAndExpand"
            HorizontalOptions="FillAndExpand" />
    </VerticalStackLayout>
</ContentPage>