<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:pages="clr-namespace:ParkingBot.Pages"
            xmlns:vm="clr-namespace:ParkingBot.ViewModels"
            xmlns:p="clr-namespace:ParkingBot.Properties"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.IsSwipePagingEnabled="False"
            x:Class="ParkingBot.Pages.MainPage"
            x:DataType="vm:MainPageVm"
            Title="{x:Static p:Lang.app_title}">
    
    <TabbedPage.BindingContext>
        <vm:MainPageVm />
    </TabbedPage.BindingContext>
    
</TabbedPage>